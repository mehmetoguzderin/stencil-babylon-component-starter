{
  "name": "stencil-starter-project-name",
  "version": "0.0.1",
  "description": "Stencil Babylon Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-starter-project-name/stencil-starter-project-name.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 stencil build --docs && npm run prettier",
    "generate": "stencil generate",
    "lint": "",
    "name": "",
    "prettier": "prettier --write src/**",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=4096 stencil build --dev --watch --serve",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=4096 stencil test --spec --e2e",
    "test.watch": "cross-env NODE_OPTIONS=--max-old-space-size=4096 stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@babylonjs/core": "preview",
    "@babylonjs/inspector": "preview",
    "@babylonjs/loaders": "preview",
    "@babylonjs/materials": "preview",
    "@stencil/core": "next",
    "@stencil/redux": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "ammo.js": "github:kripken/ammo.js",
    "cross-env": "latest",
    "earcut": "latest",
    "eslint": "latest",
    "pepjs": "latest",
    "prettier": "latest",
    "react": "latest",
    "react-dom": "latest",
    "redux": "latest",
    "rollup-plugin-node-polyfills": "latest",
    "spectorjs": "github:BabylonJS/Spector.js"
  },
  "peerDependencies": {
    "react": "latest",
    "react-dom": "latest",
    "redux": "latest"
  },
  "license": "MIT"
}
