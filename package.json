{
  "name": "stencil-starter-project-name",
  "version": "0.0.1",
  "description": "Stencil Babylon Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-starter-project-name/stencil-starter-project-name.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "name": "",
    "prettier": "prettier --write src/**",
    "build": "stencil build --docs && npm run prettier",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@babylonjs/core": "preview",
    "@babylonjs/inspector": "preview",
    "@babylonjs/loaders": "preview",
    "@babylonjs/materials": "preview",
    "@stencil/core": "next",
    "ammo.js": "github:kripken/ammo.js",
    "earcut": "latest",
    "pepjs": "latest",
    "prettier": "latest",
    "rollup-plugin-node-polyfills": "latest",
    "spectorjs": "latest"
  },
  "license": "MIT"
}
